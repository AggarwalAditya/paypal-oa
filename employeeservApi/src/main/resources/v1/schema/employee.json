{
  "title": "Employee resource",
  "description": "Employee resource object",
  "type": "object",
  "extendsJavaClass" : "Baseresponse",
  "properties": {
    "id": {
      "description": "employee id",
      "javaType":"java.lang.Long",
      "type": "object"
    },
    "first_name": {
      "description": "first name",
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "last_name": {
      "description": "last name",
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "date_of_birth": {
      "description": "date of birth",
      "type": "string",
      "pattern": "/^(0[1-9]|1[0-2])\\/(0[1-9]|1\\d|2\\d|3[01])\\/(19|20)\\d{2}$/"
    },
    "email": {
      "description": "email",
      "type": "string",
      "pattern": "^[a-zA-Z0-9+_.-]+@[a-zA-Z0-9.-]+$"
    },
    "address": {
      "type": "object",
      "properties": {
        "id": {
          "description": "address id",
          "javaType":"java.lang.Long",
          "type": "object"
        },
        "line1": {
          "description": "line1",
          "type": "string",
          "minLength": 1,
          "maxLength": 255
        },
        "line2": {
          "description": "line2",
          "type": "string",
          "minLength": 1,
          "maxLength": 255
        },
        "city": {
          "description": "city",
          "type": "string",
          "minLength": 1,
          "maxLength": 255
        },
        "state": {
          "description": "state",
          "type": "string",
          "minLength": 1,
          "maxLength": 255
        },
        "country": {
          "description": "country",
          "type": "string",
          "minLength": 1,
          "maxLength": 255
        },
        "zipcode": {
          "description": "zipcode",
          "type": "string",
          "pattern": "^[1-9][0-9]{5}$"
        }
      },
      "required": [
        "line1",
        "city",
        "state",
        "email",
        "country",
        "zipcode"
      ]
    }
  },
  "required": [
    "first_name",
    "last_name",
    "date_of_birth"
  ]
}